                        Content="{i18n:Localize ActiveLanguageManagement}"
                        IsChecked="{Binding ActiveLanguageManagement}"
                        ToolTip.Tip="{i18n:Localize ActiveLanguageManagement_Hint}">
                        <CheckBox.ContextMenu>
                            <ContextMenu>
                                <MenuItem />
                            </ContextMenu>
                        </CheckBox.ContextMenu>
                    </CheckBox>

                    <StackPanel
                        MaxWidth="400"
                        Margin="1.5"
                        IsEnabled="{Binding ActiveLanguageManagement}"
                        Orientation="Vertical">

                        <TextBlock Text="{i18n:Localize AvailableLanguages}" ToolTip.Tip="{i18n:Localize AvailableLanguages_Hint}" />

                        <ScrollViewer
                            Margin="0,7"
                            AllowAutoHide="False"
                            HorizontalScrollBarVisibility="Auto"
                            VerticalScrollBarVisibility="Disabled">
                            <StackPanel Margin="0,0,0,17" Orientation="Horizontal">
                                <ToggleButton Content="ENG" IsChecked="{Binding AvlblLngs_ENG}" />
                                <ToggleButton Content="ESP" IsChecked="{Binding AvlblLngs_ESP}" />
                                <ToggleButton Content="DEU" IsChecked="{Binding AvlblLngs_DEU}" />
                                <ToggleButton Content="FRA" IsChecked="{Binding AvlblLngs_FRA}" />
                                <ToggleButton Content="POR" IsChecked="{Binding AvlblLngs_POR}" />
                                <ToggleButton Content="ITA" IsChecked="{Binding AvlblLngs_ITA}" />
                                <ToggleButton Content="JPN" IsChecked="{Binding AvlblLngs_JPN}" />
                                <ToggleButton Content="KOR" IsChecked="{Binding AvlblLngs_KOR}" />
                                <ToggleButton Content="SIN" IsChecked="{Binding AvlblLngs_SIN}" />
                                <ToggleButton Content="CHI" IsChecked="{Binding AvlblLngs_CHI}" />
                                <ToggleButton Content="POL" IsChecked="{Binding AvlblLngs_POL}" />
                                <ToggleButton Content="RUS" IsChecked="{Binding AvlblLngs_RUS}" />
                            </StackPanel>
                        </ScrollViewer>

                        <DockPanel IsEnabled="{Binding ActiveLanguageManagement}" ToolTip.Tip="{i18n:Localize DefaultLanguage_Hint}">
                            <TextBlock Text="{i18n:Localize DefaultLanguage}" />
                            <ComboBox
                                Margin="0,1.5"
                                HorizontalAlignment="Right"
                                SelectedIndex="{Binding DefaultLanguage, Mode=TwoWay}">
                                <ComboBoxItem Content="{i18n:Localize LocalizationENG}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationESP}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationDEU}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationFRA}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationPOR}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationITA}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationJPN}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationKOR}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationSIN}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationCHI}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationPOL}" />
                                <ComboBoxItem Content="{i18n:Localize LocalizationRUS}" />
                            </ComboBox>
                        </DockPanel>

                    </StackPanel>

                </StackPanel>
            </StackPanel>


        </WrapPanel>
    </ScrollViewer>

</UserControl>
